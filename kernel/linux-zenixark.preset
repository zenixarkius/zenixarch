cat > /tmp/mkinitcpio.conf << 'CONF'
MODULES=()
BINARIES=()
FILES=()
HOOKS=(base udev encrypt)
COMPRESSION=zstd
CONF

ALL_config="/tmp/mkinitcpio.conf"
ALL_kver="/usr/lib/modules/$(pacman -Qi linux-zenixark | awk '/Version/ {print $3}')-zenixark/vmlinuz"

PRESETS=('default')
default_uki="/boot/EFI/BOOT/BOOTX64.EFI"
default_options="--cmdline /dev/null"
